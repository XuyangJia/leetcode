{
  "svn.ignoreMissingSvnWarning": true,
  "eslint.alwaysShowStatus": true,
  "eslint.nodeEnv": "",
  "eslint.packageManager": "pnpm",
  "eslint.run": "onSave",
  "eslint.format.enable": true,
  "eslint.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "prettier.arrowParens": "avoid",
